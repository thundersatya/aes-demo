      - GF_AUTH_GENERIC_OAUTH_ENABLED=true
      - GF_AUTH_GENERIC_OAUTH_ALLOW_SIGN_UP=true
      - GF_AUTH_GENERIC_OAUTH_CLIENT_ID=YOUR_CLIENT_ID
      - GF_AUTH_GENERIC_OAUTH_CLIENT_SECRET=YOUR_CLIENT_SECRET
      - GF_AUTH_GENERIC_OAUTH_SCOPES=openid profile email
      - GF_AUTH_GENERIC_OAUTH_AUTH_URL=https://your-oauth-provider.com/auth
      - GF_AUTH_GENERIC_OAUTH_TOKEN_URL=https://your-oauth-provider.com/token
      - GF_AUTH_GENERIC_OAUTH_API_URL=https://your-oauth-provider.com/userinfo
      - GF_AUTH_GENERIC_OAUTH_AUTO_ASSIGN_ORG_ROLE=Editor
